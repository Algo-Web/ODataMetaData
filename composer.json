{
  "name": "algo-web/o-data-metadata",
  "type": "library",
  "description": "OData metadata handling for PHP developers",
  "keywords": [
    "OData",
    "metadata",
    "POData"
  ],
  "homepage": "https://github.com/Algo-Web/ODataMetadata",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Goodwin",
      "email": "cyberia.resurrection@gmail.com",
      "homepage": "http://www.example.org",
      "role": "Developer"
    },
    {
      "name": "Chris Harris",
      "email": "c.harris@communicationinnovations.com.au",
      "homepage": "http://www.example.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/inflector": ">=1.1",
    "jms/serializer": "^3.0|^2.0|^1.7"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.0",
    "squizlabs/php_codesniffer": "~2.3",
    "fiunchinho/phpunit-randomizer": "2.0.*",
    "mockery/mockery": "0.9.*|^1.0@dev",
    "phpunit/phpunit": "^6.5",
    "php-coveralls/php-coveralls": ">=v2.1",
    "pcov/clobber": "dev-master"
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "autoload": {
    "psr-4": {
      "AlgoWeb\\ODataMetadata\\": "src/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {

  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
